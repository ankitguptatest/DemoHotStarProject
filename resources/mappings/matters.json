{
   "request":{
      "urlPath" :"/api/v4/matters.json",
      "method":"GET",
	  "queryParameters" : {
      "fields" : {
        "matches" : ".*"
      },
	  "status": {
		"equalTo" : "closed"
	  },
	   "close_date": {
		"matches" : "[0-9]{4}-[0-9]{2}-[0-9]{2}"
	  },
	  "access_token": {
	  "matches" : ".*"
	  },
	  "limit":{
	  "matches" : "[0-9]+"
	  },
	  "offset" :{
	   "matches" : "[0-9]+" 
   }
   }
   },
   "response":{
      "status":200,
	  "bodyFileName": "clio.json",
      "headers":{
         "Content-Type":"application/json"
      }
   }
}